{
  "name": "gatsby-starter-linting",
  "version": "0.3.4",
  "private": true,
  "description": "A starter with a single hello world component and some code quality checking tools",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-k1m/gatsby-starter-linting"
  },
  "author": "Alex Kim <45559664+Alex-K1m@users.noreply.github.com>",
  "bugs": {
    "url": "https://github.com/alex-k1m/gatsby-starter-linting/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop -H=0.0.0.0",
    "format": "prettier --check \"**/*.{json,yml,yaml,md}\"",
    "format:write": "prettier --write \"**/*.{json,yml,yaml,md}\"",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx .",
    "lint:fix": "eslint --ignore-path .gitignore --fix --ext js,jsx .",
    "postinstall": "is-ci || husky install",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "gatsby",
    "starter",
    "linting"
  ],
  "dependencies": {
    "gatsby": "^3.10.1",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-offline": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "is-ci": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "jest-preset-gatsby": "^1.0.122",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "prettier-plugin-package": "^1.3.0"
  }
}
