{
  "name": "gatsby-starter-linting",
  "version": "0.4.0",
  "private": true,
  "description": "A starter with a single hello world component and some code quality checking tools",
  "license": "0BSD",
  "repository": "git@github.com:alex-kim-dev/gatsby-starter-linting",
  "author": "Alex Kim <45559664+alex-kim-dev@users.noreply.github.com>",
  "bugs": "https://github.com/alex-kim-dev/gatsby-starter-linting/issues",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --check \"**/*.{json,yml,yaml,md}\"",
    "format:fix": "prettier --write \"**/*.{json,yml,yaml,md}\"",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx .",
    "lint:fix": "eslint --ignore-path .gitignore --fix --ext js,jsx .",
    "postinstall": "is-ci || husky install",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:watch": "jest --watch",
    "update-caniuse": "npx browserslist --update-db"
  },
  "keywords": [
    "gatsby",
    "starter",
    "linting"
  ],
  "dependencies": {
    "gatsby": "^4.3.0",
    "gatsby-plugin-image": "^2.3.0",
    "gatsby-plugin-manifest": "^4.3.0",
    "gatsby-plugin-react-helmet": "^5.3.0",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-sharp": "^4.3.0",
    "is-ci": "^3.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-preset-gatsby": "^1.0.134",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "prettier-plugin-package": "^1.3.0"
  }
}
